{
  "manifest_version": 2,
  "name": "TabSearch",
  "version": "0.2.0",
  "description": "Easy tab search & switching",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "assets/search64.png",
    "default_title": "SearchTab",
    "default_popup": "popup/index.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+L"
      },
      "description": "Execute this browser action"
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
  "icons": {
    "32": "assets/search32.png",
    "64": "assets/search64.png",
    "128": "assets/search128.png"
  },
  "minimum_chrome_version": "59.0.3071"
}