{
  "manifest_version": 2,
  "name": "TabSearch",
  "version": "0.1.1",
  "description": "Easy tab search & switching",
  "applications": {
    "gecko": {
      "strict_min_version": "52.0a1"
    }
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "icon": {
    "32": "assets/search.svg",
    "48": "assets/search.svg"
  },
  "browser_action": {
    "default_icon": "assets/search.svg",
    "default_title": "SearchTab",
    "default_popup": "popup/tab_search.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+L",
        "mac": "Command+Shift+L"
      },
      "description": "Execute this browser action"
    }
  }
}
